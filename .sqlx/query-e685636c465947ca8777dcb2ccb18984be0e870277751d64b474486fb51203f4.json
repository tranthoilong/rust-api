  "query": "\n            SELECT p.id, p.name, p.created_at, p.updated_at, p.deleted_at\n            FROM permissions p\n            INNER JOIN role_permissions rp ON p.id = rp.permission_id\n            WHERE rp.role_id = $1 AND p.deleted_at IS NULL AND rp.deleted_at IS NULL\n            ",
